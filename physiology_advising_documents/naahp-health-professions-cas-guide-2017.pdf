    <script>
      function getUniversalScreenWidth() {
        var screenWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
        return screenWidth;
      }

      function accessibilityFooter(){
        document.querySelectorAll(".h-footer-menu").forEach( function(el) {
          if(getUniversalScreenWidth() <= 1023 ) {
            el.setAttribute('tabindex', 0);
            el.removeAttribute('aria-controls');
            el.removeAttribute('aria-expanded');
          }
          else {
            el.setAttribute('tabindex', -1);
          }
        });
      }
      
      function accesibilityHeader(){
        document.querySelectorAll(".h-menu").forEach( function(el) {
          if(getUniversalScreenWidth() <= 767 ) {
            el.setAttribute('tabindex', -1);
          }
          else {
            el.setAttribute('tabindex', 0);
            el.removeAttribute('aria-controls');
            el.removeAttribute('aria-expanded');
          }
        });
      }

      function accesibilityImprovements(){
        // accesibilityHeader();
        accessibilityFooter();
      }
        
      window.addEventListener('resize', accesibilityImprovements);
      window.addEventListener('load', accesibilityImprovements);
    </script>
